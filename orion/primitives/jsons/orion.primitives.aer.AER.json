{
    "name": "orion.primitives.aer.AER",
    "contributors": [
        "Lawrence Wong <lcwong@mit.edu>"
    ],
    "documentation": "",
    "description": "autoencoder with bidirectional regression",
    "classifiers": {
        "type": "estimator",
        "subtype": "regressor"
    },
    "modalities": [],
    "primitive": "orion.primitives.aer.AER",
    "fit": {
        "method": "fit",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "y",
                "type": "ndarray"
            }
        ]
    },
    "produce": {
        "method": "predict",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "ry_hat",
                "type": "ndarray"
            },
            {
                "name": "y_hat",
                "type": "ndarray"
            },
            {
                "name": "fy_hat",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "optimizer": {
                "type": "str",
                "default": "tensorflow.keras.optimizers.Adam"
            },
            "learning_rate": {
                "type": "float",
                "default": 0.001
            },
            "reg_ratio": {
                "type": "float",
                "default": 0.5
            },
            "epochs": {
                "type": "int",
                "default": 35
            },
            "batch_size": {
                "type": "int",
                "default": 64
            },
            "lstm_units": {
                "type": "int",
                "default": 30
            },
            "callbacks": {
                "type": "list",
                "default": [
                    {
                        "class": "tensorflow.keras.callbacks.EarlyStopping",
                        "args": {
                            "monitor": "val_loss",
                            "patience": 10,
                            "min_delta": 0.0003
                        }
                    }
                ]
            },
            "validation_split": {
                "type": "float",
                "default": 0.2
            },
            "shuffle": {
                "type": "bool",
                "default": true
            },
            "verbose": {
                "type": "bool",
                "default": true
            },
            "layers_encoder": {
                "type": "list",
                "default": [
                    {
                        "class": "tensorflow.keras.layers.Bidirectional",
                        "parameters": {
                            "layer": {
                                "class": "tensorflow.keras.layers.LSTM",
                                "parameters": {
                                    "units": "lstm_units",
                                    "return_sequences": false
                                }
                            },
                            "merge_mode": "concat"
                        }
                    }
                ]
            },
            "layers_decoder": {
                "type": "list",
                "default": [
                    {
                        "class": "tensorflow.keras.layers.RepeatVector",
                        "parameters": {
                            "n": "repeat_vector_n"
                        }
                    },
                    {
                        "class": "tensorflow.keras.layers.Bidirectional",
                        "parameters": {
                            "layer": {
                                "class": "tensorflow.keras.layers.LSTM",
                                "parameters": {
                                    "units": "lstm_units",
                                    "return_sequences": true
                                }
                            },
                            "merge_mode": "concat"
                        }
                    },
                    {
                        "class": "tensorflow.keras.layers.TimeDistributed",
                        "parameters": {
                            "layer": {
                                "class": "tensorflow.keras.layers.Dense",
                                "parameters": {
                                    "units": 1
                                }
                            }
                        }
                    }
                ]
            }
        }
    }
}
