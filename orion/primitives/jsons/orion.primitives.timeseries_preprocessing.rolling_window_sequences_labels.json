{
    "name": "orion.primitives.timeseries_preprocessing.rolling_window_sequences_labels",
    "contributors": ["Sarah Alnegheimish <smish@mit.edu>"],
    "description": "Create rolling window sequences out of timeseries data.",
    "classifiers": {
        "type": "preprocessor",
        "subtype": "feature_extractor"
    },
    "modalities": [
        "timeseries"
    ],
    "primitive": "orion.primitives.timeseries_preprocessing.rolling_window_sequences_labels",
    "produce": {
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "index",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "y",
                "type": "ndarray"
            },
            {
                "name": "index",
                "type": "ndarray"
            },
            {
                "name": "target_index",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "window_size": {
                "type": "int",
                "default": 250
            },
            "target_size": {
                "type": "int",
                "default": 1
            },
            "step_size": {
                "type": "int",
                "default": 1
            },
            "target_column": {
                "type": "str or int",
                "default": 1
            },
            "min_percent": {
                "type": "float",
                "default": 0.01
            },
            "positive_class": {
                "type": "int",
                "default": 1
            }
        }
    }
}